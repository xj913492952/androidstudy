    <string name="zxing_scan_qr_code">QR Code</string>
    <style name="scaner" parent="Theme.AppCompat.Light.NoActionBar">
    </style>
    <declare-styleable name="CouponStyle">
        <attr name="vertical_style">
            <enum name="none" value="0"/>
            <enum name="circle" value="1"/>
            <enum name="triangle" value="2"/>
        </attr>
        <attr name="horizontal_style">
            <enum name="none" value="0"/>
            <enum name="circle" value="1"/>
            <enum name="triangle" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ViewfinderView">
        <attr format="color" name="corner_color"/>
        <attr format="color" name="laser_color"/>
        <attr format="color" name="frame_color"/>
        <attr format="color" name="mask_color"/>
        <attr format="color" name="result_point_color"/>
        <attr format="color" name="result_color"/>
        <attr format="color" name="label_text_color"/>
        <attr format="string" name="label_text"/>
        <attr format="dimension" name="label_text_size"/>
    </declare-styleable>
    <declare-styleable name="ZzHorizontalProgressBar">
        <attr format="dimension" name="zpb_padding"/>
        <attr format="color|reference" name="zpb_bg_color"/>
        <attr format="color|reference" name="zpb_pb_color"/>
        <attr format="integer" name="zpb_max"/>
        <attr format="integer" name="zpb_progress"/>
    </declare-styleable>
