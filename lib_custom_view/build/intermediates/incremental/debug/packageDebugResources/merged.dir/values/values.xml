<?xml version="1.0" encoding="utf-8"?>
<resources>
    <array name="week_array">
        <item>日</item>
        <item>一</item>
        <item>二</item>
        <item>三</item>
        <item>四</item>
        <item>五</item>
        <item>六</item>
    </array>
    <string name="app_name">lib_custom_view</string>
    <declare-styleable name="ArcProgress">
        <attr format="boolean" name="arc_indeterminate"/>     
        <attr format="integer" name="arc_progress"/>
        <attr format="float" name="arc_finished_start_angle"/>
        <attr format="dimension" name="arc_stroke_width"/>
        <attr format="integer" name="arc_max"/>
        <attr format="integer" name="arc_total_sweep_angle"/>
        <attr format="color" name="arc_unfinished_color"/>
        <attr format="color" name="arc_finished_color"/>
    </declare-styleable>
    <declare-styleable name="CircleProgress">
        <attr format="integer" name="circle_progress"/>
        <attr format="integer" name="circle_max"/>

        <attr format="color" name="circle_unfinished_color"/>
        <attr format="color" name="circle_finished_color"/>

        <attr format="dimension" name="circle_text_size"/>
        <attr format="color" name="circle_text_color"/>

        <attr format="string" name="circle_prefix_text"/>
        <attr format="string" name="circle_suffix_text"/>
    </declare-styleable>
    <declare-styleable name="CustomNotifyView">
        <attr format="color" name="notifyBackgroundColor"/>
        <attr format="string" name="notifyText"/>
        <attr format="color" name="notifyTextColor"/>
        <attr format="dimension" name="notifyTextSize"/>
    </declare-styleable>
    <declare-styleable name="CustomTagsLayout">
        <attr format="dimension" name="tag_margin_horizontal"/>
        <attr format="dimension" name="tag_margin_vertical"/>
    </declare-styleable>
    <declare-styleable name="HorizontalProgressBar">
        <attr format="color" name="horizontalProgressBackgroundColor"/>
        <attr format="color" name="horizontalProgressColor"/>
        <attr format="integer" name="horizontalProgress"/>
        <attr format="dimension" name="horizontalProgressRoundCornerRadius"/>
    </declare-styleable>
    <declare-styleable name="IndexBar">
        <attr format="dimension" name="letterSize"/>
        <attr format="color" name="letterColor"/>
        <attr format="color" name="focusLetterColor"/>
        <attr format="float" name="letterSpacingExtra"/>
    </declare-styleable>
    <declare-styleable name="PieChart">
        <attr format="float" name="pie_chart_finished_start_angle"/>
        <attr format="integer" name="pie_chart_max"/>
        <attr format="dimension" name="pie_chart_arc_radius"/>
        <attr format="dimension" name="pie_chart_arc_offset"/>
        <attr format="dimension" name="pie_chart_stroke_width"/>
    </declare-styleable>
    <declare-styleable name="ScanView">
        <attr format="color" name="xj_corner_color"/>
        <attr format="dimension" name="xj_corner_width"/>
        <attr format="dimension" name="xj_corner_height"/>
        <attr format="integer" name="xj_scan_speed"/>
        <attr format="integer" name="xj_refresh_time"/>
    </declare-styleable>
    <declare-styleable name="WaterPoloProgress">
        <attr format="color" name="waterPolo_circleColor"/>     
        <attr format="color" name="waterPolo_waterColor"/>      
        <attr format="color" name="waterPolo_borderColor"/>     
        <attr format="dimension" name="waterPolo_borderWidth"/> 
        <attr format="integer" name="waterPolo_startAngle"/>       
        <attr format="integer" name="waterPolo_waterPercent"/>       

        <attr format="color" name="waterPolo_centerTextColor"/>       
        <attr format="dimension" name="waterPolo_centerTextSize"/>     
        <attr format="boolean" name="waterPolo_indeterminate"/>     
    </declare-styleable>
</resources>